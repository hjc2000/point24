{
	"configurations": [
		{
			"preLaunchTask": "ninja-install",
			"name": "point24.exe",
			"type": "cppdbg",
			"request": "launch",
			"program": "${env:cpp_lib_build_scripts_path}/msys/.libs/point24/bin/point24.exe",
			"args": [
				"--numbers 2 2 4 14"
			],
			"stopAtEntry": false,
			"cwd": "${env:cpp_lib_build_scripts_path}/msys/.libs/point24/bin",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "为 gdb 启用整齐打印",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "将反汇编风格设置为 Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}
	]
}